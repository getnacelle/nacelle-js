{
  "name": "@nacelle/gatsby-source-nacelle",
  "version": "9.1.0",
  "description": "A Gatsby plugin for sourcing data from Nacelle's Hail Frequency API",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-theme",
    "nacelle",
    "ecommerce",
    "e-commerce",
    "commerce",
    "shopify",
    "magento"
  ],
  "main": "gatsby-node.js",
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint ./",
    "lockfile:update": "npm i --package-lock-only"
  },
  "author": "Nacelle Inc. (getnacelle.com)",
  "license": "Apache-2.0",
  "dependencies": {
    "@nacelle/storefront-sdk": "^1.7.10",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2"
  },
  "peerDependencies": {
    "gatsby": "^4.0.0",
    "gatsby-source-filesystem": "^4.0.0",
    "graphql": "^16.0.0"
  },
  "peerDependenciesMeta": {
    "gatsby-source-filesystem": {
      "optional": true
    }
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "lint-staged": "^10.2.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getnacelle/nacelle-js.git"
  },
  "bugs": {
    "url": "https://github.com/getnacelle/nacelle-js/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "**/*.(js|ts|tsx)": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "*{json, md}": [
      "prettier --write"
    ]
  }
}
