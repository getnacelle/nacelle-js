{
  "name": "@nacelle/sanity-plugin-nacelle-input",
  "version": "0.1.0",
  "description": "Link product & collection data from Nacelle v2 in Sanity Studio",
  "main": "dist/index.js",
  "private": true,
  "engines": {
    "node": "14.x || 16.x"
  },
  "files": [
    "dist",
    "sanity.json",
    "config.dist.json"
  ],
  "scripts": {
    "lint": "eslint ./src",
    "build": "babel src -d dist --copy-files",
    "start": "nodemon --watch src --exec 'npm run build'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getnacelle/nacelle-js.git"
  },
  "keywords": [
    "sanity",
    "plugin",
    "nacelle",
    "eCommerce",
    "e-commerce"
  ],
  "author": "Nacelle Inc. (nacelle.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/getnacelle/nacellle-js/issues"
  },
  "dependencies": {
    "isomorphic-unfetch": "^3.1.0",
    "prop-types": "^15.7.2",
    "swr": "^0.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@sanity/ui": "^0.32.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.15",
    "prettier": "^2.2.1",
    "styled-components": "^5.2.1"
  },
  "peerDependencies": {
    "@sanity/base": "^2.0.9",
    "@sanity/components": "^2.0.9",
    "@sanity/core": "^2.0.9",
    "@sanity/react-hooks": "^2.0.9",
    "@sanity/ui": "^0.32.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1"
  },
  "lint-staged": {
    "src/**/*.(js|ts)": [
      "prettier --write",
      "npm run lint"
    ],
    "*{json, md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "volta": {
    "node": "16.15.1",
    "npm": "8.13.1"
  }
}
