{
  "name": "@nacelle/nacelle-js",
  "version": "0.0.0",
  "description": "Nacelle-powered, JavaScript-fueled packages and example projects to elevate your composable commerce experience",
  "scripts": {
    "prepare": "husky install",
    "bootstrap": "lerna bootstrap --hoist --strict",
    "clean": "lerna clean",
    "create": "lerna create",
    "changed": "lerna changed",
    "format": "lerna run format",
    "lint": "lerna run lint",
    "lint:fix": "lerna run lint:fix",
    "build": "lerna run build",
    "test:ci": "jest --clearCache && lerna run test:ci",
    "release": "npm run build && lerna publish"
  },
  "engines": {
    "node": ">=14 <17",
    "npm": ">=7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getnacelle/nacelle-js.git"
  },
  "keywords": [
    "Nacelle",
    "eCommerce",
    "Headless",
    "JAMstack"
  ],
  "author": "Nacelle Inc.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/getnacelle/nacelle-js/issues"
  },
  "homepage": "https://github.com/getnacelle/nacelle-js#readme",
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-monorepo": "^0.3.2",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-vue": "^7.17.0",
    "husky": "^7.0.0",
    "jest": "^27.0.1",
    "lerna": "^4.0.0",
    "lint-staged": "^11.1.2",
    "prettier": "2.3.2",
    "tslib": "^2.3.1",
    "typescript": "4.4.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": "eslint --fix",
    "*.{js,jsx,ts,tsx,vue,css,md}": "prettier --write"
  },
  "dependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@nacelle/client-js-sdk": "^3.4.4",
    "@nacelle/types": "^5.0.2",
    "@nuxt/image": "^0.6.0",
    "@nuxtjs/composition-api": "^0.29.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@types/jest": "^27.0.1",
    "@vitejs/plugin-vue": "^1.6.1",
    "@vue/compiler-sfc": "^3.2.11",
    "@vue/composition-api": "^1.1.5",
    "@vue/test-utils": "^1.2.2",
    "@vue/vue2-jest": "^27.0.0-alpha.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^27.0.1",
    "core-js": "^3.17.3",
    "cross-fetch": "^3.1.4",
    "fibers": "^5.0.0",
    "fuse.js": "^6.4.6",
    "idb-keyval": "^6.0.2",
    "isomorphic-unfetch": "^3.1.0",
    "node-sass": "^6.0.1",
    "nuxt": "^2.15.7",
    "regenerator-runtime": "^0.11.1",
    "rollup-plugin-transform-tagged-template": "^0.0.3",
    "rollup-plugin-visualizer": "^5.5.2",
    "sass-loader": "^10.2.0",
    "serve": "^12.0.1",
    "ts-jest": "^27.0.5",
    "vite": "^2.6.2",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  }
}
